students = [
    {"name": "Ali", "grades": [80, 90, 85]},
    {"name": "Nour", "grades": [75, 88, 92]},
    {"name": "Omar", "grades": [60, 70, 65]}
]

for student in students:
    average = sum(student["grades"]) / len(student["grades"])
    print("Name:", student["name"])
    print("Average Grade:", average)
    print("-----")
